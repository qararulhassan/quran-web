                "hizbQuarter": 1,
                "sajda": false
            },
            {
                "number": 6,
                "englishText": "Guide us the straight way.",
                "arabicText": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
                "audio": "https://axntr01jcwgp.objectstorage.me-dubai-1.oci.customer-oci.com/n/axntr01jcwgp/b/quran-api/o/abdulbasit%2Fayahs%2Faudio%2F6.mp3",
