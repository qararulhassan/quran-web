            },
            {
                "number": 3,
                "englishText": "The Most Gracious, the Dispenser of Grace,",
                "arabicText": "الرَّحْمَٰنِ الرَّحِيمِ",
                "audio": "https://axntr01jcwgp.objectstorage.me-dubai-1.oci.customer-oci.com/n/axntr01jcwgp/b/quran-api/o/abdulbasit%2Fayahs%2Faudio%2F3.mp3",
