                "sajda": false
            },
            {
                "number": 4,
                "englishText": "Lord of the Day of Judgment!",
                "arabicText": "مَالِكِ يَوْمِ الدِّينِ",
                "audio": "https://axntr01jcwgp.objectstorage.me-dubai-1.oci.customer-oci.com/n/axntr01jcwgp/b/quran-api/o/abdulbasit%2Fayahs%2Faudio%2F4.mp3",
