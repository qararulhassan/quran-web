                "juz": 29,
                "manzil": 7,
                "page": 565,
                "ruku": 498,
                "hizbQuarter": 226,
                "sajda": false
            },
            {
                "number": 5295,
                "englishText": "\"Indeed, no needy person shall enter it today [and come] upon you [unawares],\"",
                "arabicText": "أَنْ لَا يَدْخُلَنَّهَا الْيَوْمَ عَلَيْكُمْ مِسْكِينٌ",
                "audio": "/API/abdulbasit/ayah/5295.mp3",
                "numberInSurah": 24,
