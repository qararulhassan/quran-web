                "arabicText": "لَيْسَ لَهَا مِنْ دُونِ اللَّهِ كَاشِفَةٌ",
                "audio": "/API/abdulbasit/ayah/4842.mp3",
                "numberInSurah": 58,
                "juz": 27,
                "manzil": 7,
                "page": 528,
                "ruku": 462,
                "hizbQuarter": 211,
                "sajda": false
            },
