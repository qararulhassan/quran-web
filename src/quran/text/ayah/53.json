                "arabicText": "فَغَشَّاهَا مَا غَشَّىٰ",
                "audio": "/API/abdulbasit/ayah/4838.mp3",
                "numberInSurah": 54,
                "juz": 27,
                "manzil": 7,
                "page": 528,
                "ruku": 462,
                "hizbQuarter": 211,
                "sajda": false
            },
