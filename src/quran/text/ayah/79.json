                "arabicText": "فَالسَّابِقَاتِ سَبْقًا",
                "audio": "/API/abdulbasit/ayah/5716.mp3",
                "numberInSurah": 4,
                "juz": 30,
                "manzil": 7,
                "page": 583,
                "ruku": 520,
                "hizbQuarter": 233,
                "sajda": false
            },
            {
